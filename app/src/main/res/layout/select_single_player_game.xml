<?xml version="1.0" encoding="utf-8"?>

<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/select_single_player_id"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="lib.game.models.fragments.Select_Single_Player_Fragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/round_corners"
        android:paddingLeft="6dp"
        android:paddingTop="4dp"
        android:paddingRight="6dp">


        <TextView
            android:id="@+id/textView4"
            android:layout_width="0dp"
            android:layout_height="20dp"
            android:layout_marginStart="5dp"
            android:layout_marginLeft="5dp"
            android:layout_marginEnd="5dp"
            android:layout_marginRight="5dp"
            android:background="@drawable/singleplayer_header"
            android:gravity="center"
            android:text="Please Select a game category"
            android:textColor="#FFFFFF"
            app:autoSizeMaxTextSize="12sp"
            app:autoSizeMinTextSize="8sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0" />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraintLayout3"
            android:layout_width="0dp"
            android:layout_height="132dp"
            android:background="#203EDF4E"
            android:paddingTop="10dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView4"
            app:layout_constraintVertical_bias="0.0">


            <Button
                android:id="@+id/nigeria_history_icon"
                android:layout_width="70dp"
                android:layout_height="65dp"
                android:layout_marginStart="10dp"
                android:layout_marginLeft="10dp"
                android:background="@drawable/africa_icon2"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.0"
                tools:layout_conversion_absoluteHeight="65dp"
                tools:layout_conversion_absoluteWidth="70dp" />

            <Button
                android:id="@+id/nigeria_sport_icon"
                android:layout_width="70dp"
                android:layout_height="65dp"
                android:background="@drawable/nigeria_sport_icon"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/nigeria_politics_icon"
                app:layout_constraintStart_toEndOf="@+id/nigeria_history_icon"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.0"
                tools:layout_conversion_absoluteHeight="65dp"
                tools:layout_conversion_absoluteWidth="70dp" />

            <Button
                android:id="@+id/nigeria_politics_icon"
                android:layout_width="70dp"
                android:layout_height="65dp"
                android:background="@drawable/nigeria_politics_icon"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.0"
                tools:layout_conversion_absoluteHeight="65dp"
                tools:layout_conversion_absoluteWidth="70dp" />

            <Button
                android:id="@+id/nigeria_entertainment_icon"
                android:layout_width="70dp"
                android:layout_height="65dp"
                android:background="@drawable/nigeria_entertainment_icon"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/nigeria_movies_icon"
                app:layout_constraintStart_toEndOf="@+id/nigeria_politics_icon"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.0"
                tools:layout_conversion_absoluteHeight="65dp"
                tools:layout_conversion_absoluteWidth="70dp" />

            <Button
                android:id="@+id/nigeria_movies_icon"
                android:layout_width="70dp"
                android:layout_height="65dp"
                android:layout_marginEnd="10dp"
                android:layout_marginRight="10dp"
                android:background="@drawable/nigeria_movies_icon"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.0"
                tools:layout_conversion_absoluteHeight="65dp"
                tools:layout_conversion_absoluteWidth="70dp" />

            <TextView
                android:id="@+id/nigeria_history_text"
                android:layout_width="70dp"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="Nigeria History"
                android:textSize="10sp"
                app:layout_constraintEnd_toEndOf="@+id/nigeria_history_icon"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="@+id/nigeria_history_icon"
                app:layout_constraintTop_toBottomOf="@+id/nigeria_history_icon" />

            <TextView
                android:id="@+id/nigeria_history_text3"
                android:layout_width="70dp"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="Nigeria politics"
                android:textSize="10sp"
                app:layout_constraintEnd_toEndOf="@+id/nigeria_politics_icon"
                app:layout_constraintStart_toStartOf="@+id/nigeria_politics_icon"
                app:layout_constraintTop_toBottomOf="@+id/nigeria_politics_icon" />

            <TextView
                android:id="@+id/nigeria_history_text5"
                android:layout_width="70dp"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="Nigeria Movies"
                android:textSize="10sp"
                app:layout_constraintEnd_toEndOf="@+id/nigeria_movies_icon"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintStart_toStartOf="@+id/nigeria_movies_icon"
                app:layout_constraintTop_toBottomOf="@+id/nigeria_politics_icon" />

            <TextView
                android:id="@+id/nigeria_history_text6"
                android:layout_width="70dp"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="Nigeria Entertainment"
                android:textSize="10sp"
                app:layout_constraintEnd_toEndOf="@+id/nigeria_entertainment_icon"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintStart_toStartOf="@+id/nigeria_entertainment_icon"
                app:layout_constraintTop_toBottomOf="@+id/nigeria_politics_icon" />

            <TextView
                android:id="@+id/nigeria_history_text4"
                android:layout_width="70dp"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="Nigeria Sport"
                app:layout_constraintEnd_toEndOf="@+id/nigeria_sport_icon"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="@+id/nigeria_sport_icon"
                app:layout_constraintTop_toBottomOf="@+id/nigeria_sport_icon"
                android:textSize="10sp" />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="0dp"
            android:layout_height="132dp"
            android:layout_marginTop="10dp"
            android:background="#203EDF4E"
            android:paddingTop="10dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/constraintLayout3">


            <Button
                android:id="@+id/nigeria_history_icon21"
                android:layout_width="70dp"
                android:layout_height="65dp"
                android:layout_marginStart="10dp"
                android:layout_marginLeft="10dp"
                android:background="@drawable/nigeria_president_icon"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.0"
                tools:layout_conversion_absoluteHeight="65dp"
                tools:layout_conversion_absoluteWidth="70dp" />

            <Button
                android:id="@+id/nigeria_history_icon22"
                android:layout_width="70dp"
                android:layout_height="65dp"
                android:background="@drawable/boko_haram_icon"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/nigeria_history_icon23"
                app:layout_constraintHorizontal_bias="0.492"
                app:layout_constraintStart_toEndOf="@+id/nigeria_history_icon21"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.0"
                tools:layout_conversion_absoluteHeight="65dp"
                tools:layout_conversion_absoluteWidth="70dp" />

            <Button
                android:id="@+id/nigeria_history_icon23"
                android:layout_width="70dp"
                android:layout_height="65dp"
                android:background="@drawable/big_brother_nigeria_icon"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.653"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.0"
                tools:layout_conversion_absoluteHeight="65dp"
                tools:layout_conversion_absoluteWidth="70dp" />

            <Button
                android:id="@+id/nigeria_history_icon25"
                android:layout_width="70dp"
                android:layout_height="65dp"
                android:layout_marginEnd="10dp"
                android:layout_marginRight="10dp"
                android:background="@drawable/africa_icon2"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.0"
                tools:layout_conversion_absoluteHeight="65dp"
                tools:layout_conversion_absoluteWidth="70dp" />

            <TextView
                android:id="@+id/nigeria_history_text21"
                android:layout_width="70dp"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="Nigeria Presidents"
                android:textSize="10sp"
                app:layout_constraintEnd_toEndOf="@+id/nigeria_history_icon21"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="@+id/nigeria_history_icon21"
                app:layout_constraintTop_toBottomOf="@+id/nigeria_history_icon21" />

            <TextView
                android:id="@+id/nigeria_history_text23"
                android:layout_width="70dp"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="Big Brother Nigeria 2020"
                android:textSize="10sp"
                app:layout_constraintEnd_toEndOf="@+id/nigeria_history_icon23"
                app:layout_constraintStart_toStartOf="@+id/nigeria_history_icon23"
                app:layout_constraintTop_toBottomOf="@+id/nigeria_history_icon23" />

            <TextView
                android:id="@+id/nigeria_history_text25"
                android:layout_width="70dp"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="How Well Do You Know Lagos"
                android:textSize="10sp"
                app:layout_constraintEnd_toEndOf="@+id/nigeria_history_icon25"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintStart_toStartOf="@+id/nigeria_history_icon25"
                app:layout_constraintTop_toBottomOf="@+id/nigeria_history_icon25" />

            <TextView
                android:id="@+id/nigeria_history_text24"
                android:layout_width="70dp"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="How Well Do You know About Boko Haram"
                android:textSize="10sp"
                app:layout_constraintEnd_toEndOf="@+id/nigeria_history_icon22"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="@+id/nigeria_history_icon22"
                app:layout_constraintTop_toBottomOf="@+id/nigeria_history_icon22" />

        </androidx.constraintlayout.widget.ConstraintLayout>


    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>